{
  "name": "LINE Clipboard",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Copy LINE stickers to your clipboard!",
  "homepage_url": "http://google.com/",
  "browser_action": {
    "default_icon": "icons/line.png",
    "default_title": "LINE Clipboard",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "http://dl.stickershop.line.naver.jp/*"
  ]
}

